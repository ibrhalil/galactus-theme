		// Token Colors
		"tokenColors": [
			{
				"name": "Comments",
				"scope": "comment",
				"settings": {
					"foreground": "#555555",
					"fontStyle": "italic"
				}
			},
			{
				"name": "Constants",
				"scope": "constant",
				"settings": {
					"foreground": "#889ca0"
				}
			},
			{
				"name": "Escape characters",
				"scope": "constant.character.escape",
				"settings": {
					"foreground": "#5e96c0"
				}
			},
			{
				"name": "Named entities",
				"scope": "entity.name",
				"settings": {
					"foreground": "#9d9ecc"
				}
			},
			{
				"name": "Tags",
				"scope": "entity.name.tag",
				"settings": {
					"foreground": "#477ab2",
					"fontStyle": ""
				}
			},
			{
				"name": "Tag attributes",
