				"settings": {
					"foreground": "#32658b",
					"fontStyle": "bold"
				}
			},
			{
				"name": "Markup italics",
				"scope": "markup.italic",
				"settings": {
					"foreground": "#267473",
					"fontStyle": "italic"
				}
			},
			{
				"name": "Markup code",
				"scope": [
