				"settings": {
					"foreground": "#9d9ecc"
				}
			},
			{
				"name": "Quoted Strings - Quotes",
				"scope": "string.quoted punctuation.definition.string",
				"settings": {
					"foreground": "#8c4665"
				}
			},
			{
				"name": "Quoted Strings - String Inside",
				"scope": "string.quoted string",
				"settings": {
					"foreground": "#8c4665"
				}
			},
			{
				"name": "Regex",
				"scope": "string.regexp",
				"settings": {
					"foreground": "#bd4d80"
				}
			},
			{
				"name": "Markup link descriptions",
				"scope": "string.other.link",
				"settings": {
					"foreground": "#899da2"
				}
			},
